    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "mongodb": "^6.3.0",
    "redis": "^4.6.10",
    "aws-sdk": "^2.1490.0",
    "@aws-sdk/client-bedrock-runtime": "^3.450.0",
