# Fin-Agentix India - Environment Configuration

# Application
NODE_ENV=development
PORT=3000
APP_NAME=Fin-Agentix India
APP_VERSION=1.0.0

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/fin_agentix
MONGODB_URL=mongodb://localhost:27017/fin_agentix_docs
REDIS_URL=redis://localhost:6379

# AWS Configuration
AWS_REGION=ap-south-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
S3_BUCKET_NAME=fin-agentix-documents
BEDROCK_MODEL_ID=amazon.titan-embed-text-v1

# Indian Government APIs
AADHAAR_UIDAI_API_KEY=your_uidai_api_key
PAN_NSDL_API_KEY=your_nsdl_api_key
GST_GSTN_API_KEY=your_gstn_api_key
DIGILOCKER_API_KEY=your_digilocker_key

# Credit Bureau APIs
CIBIL_API_KEY=your_cibil_api_key
CIBIL_API_SECRET=your_cibil_secret
EXPERIAN_API_KEY=your_experian_key
EQUIFAX_API_KEY=your_equifax_key

# Payment Gateway
UPI_GATEWAY_KEY=your_upi_gateway_key
RAZORPAY_KEY_ID=your_razorpay_key
RAZORPAY_KEY_SECRET=your_razorpay_secret

# Security
JWT_SECRET=your_super_secret_jwt_key
ENCRYPTION_KEY=your_encryption_key
BCRYPT_ROUNDS=12

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@domain.com
SMTP_PASS=your_email_password

# External Services
GOOGLE_MAPS_API_KEY=your_google_maps_key
TWILIO_ACCOUNT_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_token

# Monitoring
DATADOG_API_KEY=your_datadog_key
SENTRY_DSN=your_sentry_dsn

# Feature Flags
ENABLE_AI_AGENTS=true
ENABLE_REAL_TIME_SCORING=true
ENABLE_BIOMETRIC_KYC=true
ENABLE_VERNACULAR_SUPPORT=true
